#!/bin/sh

sleep 5
dbus-update-activation-environment --systemd PATH XDG_CURRENT_DESKTOP XDG_DATA_DIRS XDG_MENU_PREFIX XDG_RUNTIME_DIR XDG_SEAT XDG_SESSION_DESKTOP XDG_SESSION_ID XDG_SESSION_TYPE XDG_VTNR SSH_AUTH_SOCK DBUS_SESSION_BUS_ADDRESS
# systemctl --user import-environment PATH
systemctl --user start started.target
